meta::configure 'alias', inherit => 0;
meta::define_form 'alias', sub {
  my ($name, $value) = @_;
  my @pre_tokenized  = shell::tokenize($value);
  meta::externalize $name, "alias::$name", sub {
    shell::run(@pre_tokenized, shell::tokenize(@_));
  };
};
